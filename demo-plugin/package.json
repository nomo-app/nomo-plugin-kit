{
  "name": "nomo-demo-plugin",
  "scripts": {
    "dev": "next dev",
    "deploy_legacy": "rm -f .env.local && ./scripts/deploy_legacy.sh",
    "deploy_zip": "./scripts/deploy_zip.sh",
    "build": "next build && next export",
    "build_tar": "npm run build && tar -czvf $npm_package_name.tar.gz out",
    "build_zip": "npm run build && rm -f $npm_package_name.zip && zip -r $npm_package_name.zip out",
    "start": "npx serve@latest out",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/node": "20.2.3",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.3",
    "ethersjs-nomo-plugins": "file:../ethersjs-nomo-plugins",
    "next": "13.4.3",
    "nomo-plugin-kit": "file:../nomo-plugin-kit",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.4",
    "web3js-nomo-plugins": "file:../web3js-nomo-plugins"
  }
}
